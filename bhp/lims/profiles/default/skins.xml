<?xml version="1.0"?>
<object name="portal_skins">

    <object name="bhp_images"
            meta_type="Filesystem Directory View"
            directory="bhp.lims:skins/bhp_images"/>

    <object name="bhp_scripts"
            meta_type="Filesystem Directory View"
            directory="bhp.lims:skins/bhp_scripts"/>

    <object name="bhp_widgets"
            meta_type="Filesystem Directory View"
            directory="bhp.lims:skins/bhp_widgets"/>

    <skin-path name="*">
        <layer name="bhp_images" insert-after="custom"/>
        <layer name="bhp_scripts" insert-after="bhp_images"/>
        <layer name="bhp_widgets" insert-after="bhp_scripts"/>
    </skin-path>

</object>
