<?xml version="1.0" encoding="UTF-8"?>
<object name="ReferralLabs"
        meta_type="Factory-based Type Information with dynamic views"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="bhp.lims"
        purge="True">

  <property name="title" i18n:translate="">Referral Laboratories</property>
  <property name="content_icon">++resource++bhp.lims.static/images/referrallabs.png</property>
  <property name="content_meta_type">ReferralLabs</property>
  <property name="product">bhp.lims</property>
  <property name="factory">addReferralLabs</property>
  <property name="global_allow">False</property>
  <property name="filter_content_types">True</property>
  <property name="allowed_content_types">
    <element value="ReferralLab"/>
  </property>
  <property name="allow_discussion">False</property>

  <!-- View information -->
  <property name="default_view_fallback">False</property>

  <alias from="(Default)" to="folder_view"/>
  <alias from="base_view" to="folder_view"/>
  <alias from="base_edit" to="folder_view"/>
  <alias from="view" to="folder_view"/>
  <alias from="edit" to="folder_view"/>

  <!-- Actions -->

</object>
